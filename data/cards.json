---
---
[
  {% assign cards = site.data.cardsdb %}
  {% for card in cards %}
    {% unless card[1].desc contains "Pendulum Effect" or card[1].desc contains "Pendulum Monster" %}
      {
        "name": "{{card[1].name | replace: '"', '\\"'}}",
        "nameSearch": "{{card[1].name | replace: '"', "" | downcase}}",
        "nameLink": "{{card[1].name | replace: ' ', "_" | replace: '"', "_" | replace: 'â€™', "'" | replace: '!', "_" | remove: '#'}}",
        "description": "{{card[1].desc | strip_tabs | strip_newlines | escape_backslashes | replace: '"', "" | downcase}}"
      }
      {% unless forloop.last %},{% endunless %}
    {% endunless %}
  {% endfor %}
]