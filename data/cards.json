[
  {% assign cards = site.data.cardsdb %}
  {% for card in cards %}
    {
      "name": "{{card[1].name | replace: '"', "'"}}",
      "description": "{{card[1].desc | strip_tabs | strip_newlines | escape_backslashes | replace: '"', "'"}}"
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
]